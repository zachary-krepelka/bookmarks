{
  "manifest_version": 3,
  "name": "Bookmarks Bar Enumerator",
  "description": "Enumerate Items on the Bookmarks Bar",
  "homepage_url": "https://github.com/zachary-krepelka/bookmarks/tree/main/extensions/bookmarks-bar-enumerator",
  "version": "1.0",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "background": {
    "service_worker": "service-worker.js"
  },
  "action": {
    "default_icon": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    }
  },
  "permissions": [
    "bookmarks"
  ]
}
